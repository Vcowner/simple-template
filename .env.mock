# ============================================
# Mock 模式环境变量配置（使用本地 Mock 数据）
# ============================================

# 环境标识（用于插件判断）
VITE_USER_NODE_ENV=development

# API 基础路径（用于 API 请求的 baseURL）
VITE_API_BASE_URL=/api

# 开发服务器配置
VITE_PORT=8080
VITE_OPEN=true

# 代理配置（Mock 模式下禁用代理，使用本地 Mock 数据）
# 格式：[{"prefix":"/api","target":"http://localhost:3000","pathRewrite":""}]
# VITE_PROXY=[{"prefix":"/api","target":"http://172.16.12.60:8000","pathRewrite":""}]

# 预览服务器端口
VITE_PREVIEW_PORT=4173

# 构建配置
VITE_DROP_CONSOLE=false
VITE_MINIFY=esbuild
VITE_REPORT=false
