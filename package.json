{
  "name": "power-iot-traffic",
  "version": "1.0.0",
  "description": "电力物联终端设备流量特征识别系统",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{vue,js,ts,tsx}\" --max-warnings=0",
    "lint:style": "stylelint \"src/**/*.{vue,css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,json,css,scss,md,vue}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss,vue}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "template",
    "frontend",
    "html",
    "css",
    "javascript",
    "vite",
    "vue",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "4.2.3",
    "axios": "1.6.8",
    "dayjs": "1.11.11",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.2.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.93.3",
    "sass-embedded": "^1.75.0",
    "stylelint": "^16.26.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-vue-devtools": "^7.0.0",
    "vue-tsc": "^2.0.6"
  }
}
