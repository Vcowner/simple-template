{
  "name": "power-iot-traffic",
  "version": "1.0.0",
  "description": "电力物联终端设备流量特征识别系统",
  "type": "module",
  "main": "index.html",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "start": "vite",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{js,ts,json,css,scss,md,vue}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "template",
    "frontend",
    "html",
    "css",
    "javascript",
    "vite",
    "vue",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/vue-router": "^2.0.0",
    "ant-design-vue": "4",
    "axios": "^1.6.0",
    "dayjs": "^1.11.19",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "mockjs": "^1.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "sass": "^1.93.3",
    "sass-embedded": "^1.75.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-vue-devtools": "^7.0.0",
    "vue-tsc": "^2.0.0"
  }
}
